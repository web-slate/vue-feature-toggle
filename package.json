{
  "name": "vue-feature-toggle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:us": "COUNTRY='US' npm run serve",
    "serve:sg": "COUNTRY='SG' npm run serve",
    "serve:in": "COUNTRY='IN' npm run serve",
    "build": "vue-cli-service build",
    "build:us": "COUNTRY='US' npm run build",
    "build:sg": "COUNTRY='SG' npm run build",
    "build:in": "COUNTRY='IN' npm run build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "3.10.1",
    "vue": "3.0.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.12",
    "@vue/cli-plugin-eslint": "4.5.12",
    "@vue/cli-service": "4.5.12",
    "@vue/compiler-sfc": "3.0.11",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "7.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
